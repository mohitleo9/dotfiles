<?xml version="1.0"?>
<root>
    <item>
        <name>f19 to f19</name>
        <appendix>(+ When you type f19 only, send Escape)</appendix>
        <identifier>private.f19_to_f19_and_escape</identifier>
        <autogen>__KeyOverlaidModifier__ KeyCode::F19, KeyCode::OPTION_R, KeyCode::ESCAPE</autogen>
    </item>
    <item>
        <name>f19 + hjkl</name>
        <appendix>(when keep f19 pressed hjkl act like vim navigation)</appendix>
        <identifier>private.f19_to_navigate_like_vim</identifier>
        <autogen>__KeyToKey__
            KeyCode::J, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_DOWN,
        </autogen>
        <autogen>__KeyToKey__
            KeyCode::H, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_LEFT,
        </autogen>
        <autogen>__KeyToKey__
            KeyCode::K, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_UP,
        </autogen>
        <autogen>__KeyToKey__
            KeyCode::L, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_RIGHT,
        </autogen>
    </item>
    <item>
        <name>f19 + wbae</name>
        <appendix>(when keep f19 pressed wb0$ act like vim navigation)</appendix>
        <identifier>private.f19_to_navigate_like_vim_more</identifier>
        <autogen>__KeyToKey__
            KeyCode::W, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_RIGHT, ModifierFlag::OPTION_L
        </autogen>
        <autogen>__KeyToKey__
            KeyCode::B, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_LEFT, ModifierFlag::OPTION_L
        </autogen>
        <autogen>__KeyToKey__
            KeyCode::A, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L
        </autogen>
        <autogen>__KeyToKey__
            KeyCode::E, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L
        </autogen>
    </item>
    <item>
        <name>1234567890 to !@#$%^&amp;*() </name>
        <identifier>private.swap_number_and_symbol</identifier>
        <autogen>__KeyToKey__ KeyCode::KEY_1, ModifierFlag::NONE, KeyCode::KEY_1, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_2, ModifierFlag::NONE, KeyCode::KEY_2, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_3, ModifierFlag::NONE, KeyCode::KEY_3, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_4, ModifierFlag::NONE, KeyCode::KEY_4, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_5, ModifierFlag::NONE, KeyCode::KEY_5, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_6, ModifierFlag::NONE, KeyCode::KEY_6, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_7, ModifierFlag::NONE, KeyCode::KEY_7, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_8, ModifierFlag::NONE, KeyCode::KEY_8, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_9, ModifierFlag::NONE, KeyCode::KEY_9, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::KEY_0, ModifierFlag::NONE, KeyCode::KEY_0, ModifierFlag::SHIFT_L</autogen>
    </item>
    <item>
        <name>Home Row Numbers Overlay with rightCmd</name>
        <item>
            <name>Home Row Numbers</name>
            <appendix>Overlay 1234567890 onto the home row but dont select this one (the next one)(ASDFGHJKL;)</appendix>
            <identifier vk_config="true">private.notsave.homerow_numbers</identifier>
            <autogen>__KeyToKey__ KeyCode::A, KeyCode::KEY_1</autogen>
            <autogen>__KeyToKey__ KeyCode::S, KeyCode::KEY_2</autogen>
            <autogen>__KeyToKey__ KeyCode::D, KeyCode::KEY_3</autogen>
            <autogen>__KeyToKey__ KeyCode::F, KeyCode::KEY_4</autogen>
            <autogen>__KeyToKey__ KeyCode::G, KeyCode::KEY_5</autogen>
            <autogen>__KeyToKey__ KeyCode::H, KeyCode::KEY_6</autogen>
            <autogen>__KeyToKey__ KeyCode::J, KeyCode::KEY_7</autogen>
            <autogen>__KeyToKey__ KeyCode::K, KeyCode::KEY_8</autogen>
            <autogen>__KeyToKey__ KeyCode::L, KeyCode::KEY_9</autogen>
            <autogen>__KeyToKey__ KeyCode::SEMICOLON, KeyCode::KEY_0</autogen>
        </item>
        <item>
            <name>Hold RCOMMAND+ASDFGHJKL; to 1234567890</name>
            <identifier>private.Right_cmd_to_homerow_numbers</identifier>
            <autogen>__KeyOverlaidModifier__ KeyCode::COMMAND_R, KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_private_notsave_homerow_numbers, KeyCode::SPACE</autogen>
        </item>
    </item>
</root>
